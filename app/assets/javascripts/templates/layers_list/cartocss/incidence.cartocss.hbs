/** choropleth visualization */

#crm_globocan_2012 {
  polygon-fill: transparent;
  polygon-opacity: 0.8;
  line-color: #FFF;
  line-width: 1;
  line-opacity: 1;
}
#crm_globocan_2012 [ value > 242.9] {
  polygon-fill: #CD0000;
}
#crm_globocan_2012 [ value <= 242.9] {
  polygon-fill: #D84948;
}
#crm_globocan_2012 [ value <= 172.3] {
  polygon-fill: #E18683;
}
#crm_globocan_2012 [ value <= 137.5] {
  polygon-fill: #E8B7B3;
}
#crm_globocan_2012 [ value <= 101.3] {
  polygon-fill: #E9C0BC;
}
