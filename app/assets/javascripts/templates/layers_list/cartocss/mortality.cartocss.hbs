/** choropleth visualization */

#crm_globocan_2012 {
  polygon-fill: transparent;
  polygon-opacity: 0.8;
  line-color: #FFF;
  line-width: 1;
  line-opacity: 1;
}
#crm_globocan_2012 [ value > 116.8] {
  polygon-fill: #844300;
}
#crm_globocan_2012 [ value <= 116.8] {
  polygon-fill: #A47848;
}
#crm_globocan_2012 [ value <= 99.6] {
  polygon-fill: #BFA483;
}
#crm_globocan_2012 [ value <= 89.7] {
  polygon-fill: #D3C9B1;
}
#crm_globocan_2012 [ value <= 73.3] {
  polygon-fill: #D9CFBC;
}
